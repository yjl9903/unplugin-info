{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "lib": ["esnext", "dom"],
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true
  }
}
